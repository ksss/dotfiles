[alias]
	s = status
	c = checkout
	b = branch
	l = log --decorate
	g = grep
	bm = branch --merged
	commit = commit -v
	tree = log --graph --pretty=oneline --abbrev-commit --decorate
	stat = diff --stat
	ls-files-exclude-other = ls-files --exclude-standard --others
[color]
	ui = true
[branch]
	autosetuprebase = always
